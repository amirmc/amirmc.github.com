<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>{{ site.title }} - 'MISO'</title>
  <link href="{{ site.production_url }}/tags/miso-atom.xml" rel="self"/>
  <link href="{{ site.production_url }}"/>
  <subtitle>{{ site.tagline }}</subtitle>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>{{ site.production_url }}/tags/#miso</id>
  <author>
    <name>{{ site.author.name }}</name>
  </author>
  {% for post in site.tags.miso %}
    {% include amc/atom_entry.xml %}
  {% endfor %}
</feed>
